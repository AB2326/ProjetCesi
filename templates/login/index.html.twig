{% extends 'base.html.twig' %}

{% block title %}Login{% endblock %}

{% block body %}
    <h1>Login</h1>

    {% if error %}
        <div>{{ error }}</div>
    {% endif %}

        {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.firstPassword) }}
        <button type="submit">Login</button>
        {{ form_end(form) }}
{% endblock %}
